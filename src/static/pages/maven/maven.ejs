      height: "167",
      weight: "60",
      hair: "Dark Brown",
      eyes: "Green",
      dislikes: "Bla, Stupid jokes, Bla"
    }) %>
    
    <% const skills = [
      { skillName: "Responsibility", color: "red", percentage: "90" },
      { skillName: "Calmness", color: "pink", percentage: "35" },
      { skillName: "Creativity", color: "yellow", percentage: "95" },
      { skillName: "Communication", color: "green", percentage: "75" }            
   ]; %>

    <section class="block features">
      <div class="block__wrapper">
        <h2 class="features__title">Personality</h2>
        <ul class="features__list">
          <% for (let skill of skills) { %>
            <li class="features__item">
              <%- include('../../shared/components/skill/skill.ejs', { skillName:
                skill.skillName, color: skill.color, percentage: skill.percentage }) %>
            </li>
          <% } %>
        </ul>
      </div>
    </section>
